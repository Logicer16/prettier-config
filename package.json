{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@logicer/prettier-config",
  "version": "1.0.2",
  "description": "Logicer's Prettier Base Configuration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "style": "npm run lint && npm run prettier",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "lint:ci": "eslint 'src/**/*.{js,ts}'",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Logicer16/prettier-config.git"
  },
  "author": "logicer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Logicer16/prettier-config/issues"
  },
  "homepage": "https://github.com/Logicer16/prettier-config#readme",
  "engines": {
    "node": "^18.14.2"
  },
  "devDependencies": {
    "@logicer/eslint-plugin": "^1.2.0",
    "@logicer/prettier-config": "^1.0.1",
    "@logicer/tsconfig": "^3.0.0",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-plugin": "^5.1.0",
    "eslint-plugin-jsdoc": "^46.4.3",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "3.0.3",
    "typescript": "^5.0.4"
  }
}
